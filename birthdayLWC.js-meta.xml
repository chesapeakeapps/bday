<?xml version="1.0" encoding="UTF-8"?>
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>49.0</apiVersion>
    <!-- isExposed must be true so that this LWC can be used within Salesforce. -->
    <isExposed>true</isExposed>
    <description>Contact Birthday Tracker LWC component used to track contact birthdays.</description>
    <targets>
    <!-- Targets are set so that this LWC can be used for all 3 page types within Lightning App Builder. -->
    <target>lightning__HomePage</target>
    <target>lightning__AppPage</target>
    <target>lightning__RecordPage</target>
    </targets>
</LightningComponentBundle>